{
    "name": "john-jun/FastCGI",
    "description": "A simple FastCGI protocol",
    "keywords": ["Air", "FastCgi", "cgi"],
    "license": "MIT",
    "authors": [
        {
            "name": "John",
            "email": "53950451@qq.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "john-jun/socket-client": "^1.0"
    },
    "scripts": {
        "test": "phpunit --colors=always tests"
    },
    "autoload": {
        "psr-4": {
